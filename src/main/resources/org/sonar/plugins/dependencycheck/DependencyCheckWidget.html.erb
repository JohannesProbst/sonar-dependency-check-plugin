<p class="title">
	<b>Dependency Check</b>
</p>

<% measure = format_measure('dependencycheck', :default => 'test')

dependencies = measure.split(';') %>

<p>
	<%= measure %>
	test
</p>


<table class="width100">
	<tbody>
		<tr>
			<th style="text-align: center"><b>Library</b></th>
			<th style="text-align: center"><b>License</b></th>
			<th style="text-align: center"><b>Status</b></th>
		</tr>

		<% 	dependencies.each do |dependency| 
			d = dependency.split(',') 
		%>
		<tr>
			<td><%= d[0] %></td>
			<td><%= d[1] %></td>
			<td><%= d[2] %></td>
		</tr>
		
		<% end %>
	</tbody>
</table>
